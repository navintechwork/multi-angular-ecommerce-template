{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app-one": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "projects/app-one",
      "sourceRoot": "projects/app-one/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/app-one",
            "index": "projects/app-one/src/index.html",
            "browser": "projects/app-one/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/app-one/tsconfig.app.json",
            "assets": [
              "projects/app-one/src/favicon.ico",
              "projects/app-one/src/assets"
            ],
            "styles": [
              "projects/app-one/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app-one:build:production"
            },
            "development": {
              "buildTarget": "app-one:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "app-one:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/app-one/tsconfig.spec.json",
            "assets": [
              "projects/app-one/src/favicon.ico",
              "projects/app-one/src/assets"
            ],
            "styles": [
              "projects/app-one/src/styles.css",
              "projects/app-one/src/assets/css/demo10.min.css",
              "projects/app-one/src/assets/css/app.min.css"
            ],
            "scripts": [
              "projects/app-one/src/assets/js/main.min.js"
            ]
          }
        }
      }
    },
    "app-two": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "projects/app-two",
      "sourceRoot": "projects/app-two/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/app-two",
            "index": "projects/app-two/src/index.html",
            "browser": "projects/app-two/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/app-two/tsconfig.app.json",
            "assets": [
              "projects/app-two/src/favicon.ico",
              "projects/app-two/src/assets"
            ],
            "styles": [
              "projects/app-two/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app-two:build:production"
            },
            "development": {
              "buildTarget": "app-two:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "app-two:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
