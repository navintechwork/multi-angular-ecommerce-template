            "tsConfig": "projects/app-two/tsconfig.spec.json",
            "assets": [
              "projects/app-two/src/favicon.ico",
              "projects/app-two/src/assets"
            ],
            "styles": [
              "projects/app-two/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
